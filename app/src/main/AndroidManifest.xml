<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.russell.utad">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Home_Screen"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CallConfig_Screen"></activity>
        <activity android:name=".Contacts_Screen"></activity>
        <activity android:name=".DriveMode_Screen"></activity>
        <activity android:name=".Help_Screen"></activity>
        <activity android:name=".SpecialTextConfig_Screen"></activity>
        <activity android:name=".Settings_Screen"></activity>
        <activity android:name=".TextConfig_Screen"></activity>
    </application>

</manifest>
